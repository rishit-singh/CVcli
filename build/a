import os
import json
import sys

class FileIO:
    DefaultTokenFile = "~/.tokens.json"
    
    def SetTokenFile():
        try:
            FileIO.DefaultTokenFile = sys.argv["TOKEN_FILE"]
        except:
            return
    
    def ReadFile(path: str):
        try:
            return open(path, 'r').read()
        except FileNotFoundError:
            print(f"File {path} not found.")
        return str()

class Token:
    Key = str()
    Value = str()
    def __init__(self, key: str, value: str):
        self.Key = key
        self.Value = value

    def ToBashString(self) -> str:
        return str(f"export {self.Key}={self.Value}")
    def Export(self) -> bool:
        return os.system()

class TokenManager:
    def GetTokens()
    def ExportTokens(tokens: list[Token]):
         
